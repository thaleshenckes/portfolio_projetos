        "            'CompetitionOpenSinceYear', 'Promo2', 'Promo2SinceWeek','Promo2SinceYear', 'PromoInterval']\n",
        "\n",
        "snakecase = lambda x: inflection.underscore(x)\n",
        "\n",
        "cols_new = list( map (snakecase, cols_old))\n",
        "\n",
        "df1.columns = cols_new"
